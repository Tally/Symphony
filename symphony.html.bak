<!DOCTYPE html>
	<head>
	<title>Symphony</title>
	<link rel='stylesheet' type='text/css' href='symphony.css'>
	<script src='jquery-1.10.2.min.js'></script>
    <script src='symphony.js'></script>
	</head>
	<body>
	<div id='wrapper'>
		<div id='titlebar'>
		<h1>Symphony</h1>
		</div>
	<div id='userpanel'>
		<span id='username'>Prally<br>
		Lassark</span>
		<div id='ownlast3'>
		<h4>Recently Submitted</h4>
		<table width='100px' id='submissions'><tr>
			<td><img title='LCD Soundsystem 'src='lcd.jpg'></td>
			<td><img title='Nirvana' src='nirvana.jpg'></td>
			<td><img title='The Tins' src='tins.jpg'></td>
			</tr></table>
			<button id='submitnew'>Submit</button>
		</div>
		<div id='controls'>
		<h5> Symphonic Friends </h5>
		<ul id='friendlist'>
		</ul>
		</span>
		</div>
	</div>
	<div id='contentpanel'>
		<div id='musicarea'>
		<div id='top3'>	
			<div class='recommendationtitle' id='top3title'>Your Top 3 Recommendations</div>
			<div id='top3summary'></div>
			<div id='top3details'>
				<table>
					<tr>
						<td colspan='2' class='recommenderCell'></td>
					</tr><tr>
						<td rowspan='5' class='coverCell'></td>
						<td class='titleCell'></td>
					</tr><tr>
						<td class='linkCell'></td>
					</tr><tr>
						<td class='reviewCell'></td>
					</tr><tr>
						<td class='tagCell'>Tags?</td>
					</tr><tr>
						<td class='buttonCell'><button type='button' class='backbutton'><em>Back</em></button></td>
					</tr>
				</table>
			</div>
			<div id='submissionform'>
				<table>
				<tr>
					<td>
						<select name='trackoralbum'>
						<option value='track'>Track</option>
						<option value='album'>Album</option>
						<option value='general'>General</option>
						</select>
						<input type='text' name='title'></td>
					<td>by <input type='text' name='artist'></td>
				</tr><tr>
					<td><input type='text' name='imagesource'</td>
					<td><input type='text' name='sourcelink'</td>
				</tr><tr>
					<td colspan='2'><textarea name='review'></textarea></td>
				</tr><tr>	
					<td colspan='2'><input type='text' name='tags'></td>
				</tr><tr>
					<td><input type='text' name='recommendees'></td>
					<td><button type='button' id='submitconfirm'><em>Submit Recommendation</em></button><button type='button' id='submitcancel'>Cancel</button></td>
				</tr>
				</table>
			</div>
		</div>
		<div id='next5'>
			<div class='recommendationtitle' id='next5title'>5 More Recommendations</div>
			<div id='prev5recommendations'>-</div>
			<div id='next5recommendations'>+</div>
			<div id='next5summary'><table><tr>
				<td class='coverCell'></td>
				<td class='detailCell'></td>
				<td class='coverCell'></td>
				<td class='detailCell'></td>
				<td class='coverCell'></td>
				<td class='detailCell'></td>
				<td class='coverCell'></td>
				<td class='detailCell'></td>
				<td class='coverCell'></td>
				<td class='detailCell'></td>
			</tr></table></div>
		</div>
		<div id='dynamicwell'>
			<div class='recommendationtitle' id='welltitle'>Past Music Recommended by Tally</div>
			<table><tr>
			<td class='cover1'><img src='well1.jpg' title='Grimes' ></td>
			<td class='cover1'><img src='well2.jpg' title='Blue Sky Black Death'></td>
			<td class='cover1'><img src='well3.jpg' title='Twin Shadow'></td>
			<td class='cover1'><img src='well4.jpg' title='RedLine Soundtrack'></td>
			<td class='cover1'><img src='well5.jpg' title='Phantogram'></td>
			</table>
			</div>
		</div>
		</div>
	</div>
	</body>
</html>